@inject NavigationManager NavigationManager
<ion-header>
    <ion-toolbar color="primary">
        <ion-title>Menu</ion-title>
    </ion-toolbar>
</ion-header>

<ion-content>
    <ion-list>
        <ion-list-header>
            Navigate
        </ion-list-header>
        <ion-menu-toggle auto-hide="false">
            <IonItem href="">
                <ion-icon slot="start" name='home'></ion-icon>
                <ion-label>
                    Home
                </ion-label>
            </IonItem>
            <IonItem href="demo">
                <ion-icon name="add-circle-outline" slot="start"></ion-icon>
                <ion-label>Ionic Demo</ion-label>
            </IonItem>
            <IonItem href="inputs">
                <ion-icon name="add-circle-outline" slot="start"></ion-icon>
                <ion-label>Inputs</ion-label>
            </IonItem>
            <IonItem href="tabs">
                <ion-icon name="add-circle-outline" slot="start"></ion-icon>
                <ion-label>Tabs</ion-label>
            </IonItem>
            <IonItem href="select">
                <ion-icon name="add-circle-outline" slot="start"></ion-icon>
                <ion-label>Select</ion-label>
            </IonItem>
        </ion-menu-toggle>
    </ion-list>
</ion-content>